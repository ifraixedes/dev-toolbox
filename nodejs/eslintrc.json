{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "standard",
  "rules": {
    "block-scoped-var": 2,
    "callback-return": 2,
    "consistent-return": 2,
    "consistent-this": [ 2, "self" ],
    "dot-location": [ 2, "property" ],
    "dot-notation": 2,
    "global-require": 2,
    "guard-for-in": 2,
    "linebreak-style": [ 2, "unix" ],
    "max-nested-callbacks": [ 2, 2 ],
    "no-catch-shadow": 2,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-div-regex": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-implicit-coercion": 2,
    "no-invalid-this": 2,
    "no-lonely-if": 2,
    "no-nested-ternary": 2,
    "no-process-env": 2,
    "no-process-exit": 2,
    "no-script-url": 2,
    "no-shadow": [ 2, { "builtinGlobals": true, "hoist": "functions" } ],
    "no-underscore-dangle": 2,
    "no-unneeded-ternary": 2,
    "no-unused-expressions": 2,
    "no-use-before-define": [ 2, { "functions": false } ],
    "no-useless-concat": 2,
    "no-var": 0,
    "no-void": 2,
    "no-warning-comments": [ 1, { "terms": [ "todo", "fixme", "refactor", "refactoring", "crap" ], "location": "anywhere" } ],
    "object-shorthand": 0,
    "operator-assignment": [ 2, "always" ],
    "prefer-arrow-callbacks": 0,
    "prefer-const": 0,
    "prefer-reflect": 0,
    "prefer-spread": 0,
    "prefer-template": 2,
    "quote-props": [ 2, "as-needed" ],
    "require-yield": 2,
    "valid-jsdoc": [ 2, { "prefer": { "return": "returns", "arg": "param", "argument": "param" }, "requireReturn": false } ]
  }
}
